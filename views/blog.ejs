<% include partials/header %>
<style>
    .content * {
        margin-right: auto;
        margin-left: auto;
    }
    .content {
        text-align: center;
    }
</style>
<h1 class="title">Blog</h1>
<%for(var i = 0; i < docs.length; i++){%>
    <div class="shade result" style="width:60vw;background-image:url(/img/<%=docs[i].image%>);background-size:100%">
        <a href="/posts/<%=docs[i].postname%>">
            <h1><%=docs[i].name%></h1>
            <h2><%=docs[i].content%></h2>
        </a>
    </div>
<%}%>
<% include partials/footer %>